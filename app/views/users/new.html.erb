<h1>ログイン</h1>
<%= form_with model: @user, local: true do |f| %>
    <div>
    <label>名前</label>
    <%= f.text_field :name %>
    </div>
    <div>
    <label>メールアドレス</label>
    <%= f.text_field :email %>
    </div>
    <div>
    <label>パスワード</label>
    <%= f.password_field :password %>
    </div>
    <div>
        <label>パスワード再入力</label>
        <%= f.password_field :password_confirmation %>
    </div>
    <div>
        <label>プロフィール文</label><br>
        <%= f.text_area :profile %>
    </div>
    <div>
    <label>プロフィール画像の設定</label>
    <%= f.file_field :image %>
    </div>
    <div>
    <%= f.submit "登録", class: "btn btn-primary" %>
    </div>
<% end %>